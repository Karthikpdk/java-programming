import java.util.*;
class Main {
    public static void main(String[] args) {
        String data = "eye";
        String temp = "";
        int len = data.length() - 1;
        while (len >= 0) {
            temp = temp + data.charAt(len);
            len--;
        }
        if (data.equals(temp)) {
            System.out.println("Palindrome");
        } else {
            System.out.println("Not a palindrome");
        }
    }
}
